{
  "compilerOptions": {
    "module": "NodeNext",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/grpc": [
        "libs/grpc/src"
      ],
      "@app/grpc/*": [
        "libs/grpc/src/*"
      ],
      "@libs/consul": [
        "libs/consul/src"
      ],
      "@libs/consul/*": [
        "libs/consul/src/*"
      ]
    }
  }
}